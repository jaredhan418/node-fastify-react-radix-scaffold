generator client {
  provider = "prisma-client"
  output   = "./generated/prisma"
}

datasource db {
  provider = "mysql"
}

model short_origin_table {
  shortlinkId  String   @db.VarChar(8)
  originalLink String   @db.Text
  createTime   DateTime @db.Timestamp(0)
  id           Int      @id @default(autoincrement())
}
