{
  "name": "node-fastify-react-radix-scaffold",
  "version": "0.5.0",
  "description": "Node fastify Prisma scaffold",
  "main": "dist/server/index.js",
  "type": "module",
  "scripts": {
    "build": "npm run build:browser && npm run build:server",
    "build:browser": "vite build",
    "build:server": "tsc --project src/server/tsconfig.json",
    "clean": "rimraf dist",
    "inspect": "npx @modelcontextprotocol/inspector",
    "husky:pre-push": "npm test",
    "husky:pre-commit": "npx --no lint-staged",
    "husky:commit-msg": "npx --no commitlint -- --edit",
    "start:server": "node --env-file=.env ./dist/index.js",
    "start:browser": "vite dev",
    "watch:build:server": "npm run build:server -- --watch --incremental --sourceMap --preserveWatchOutput",
    "test": "vitest run",
    "prepare": "npm run clean && npx --no prisma generate && npm run build && npx --no husky"
  },
  "keywords": [
    "node",
    "short-link"
  ],
  "author": "Han Jiawei",
  "license": "MIT",
  "dependencies": {
    "@fastify/compress": "^8.1.0",
    "@fastify/cookie": "^11.0.2",
    "@fastify/http-proxy": "^11.3.0",
    "@fastify/session": "^11.1.0",
    "@fastify/static": "^8.3.0",
    "@fastify/swagger": "^9.5.2",
    "@fastify/swagger-ui": "^5.2.3",
    "@prisma/client": "^6.18.0",
    "connect-redis": "^9.0.0",
    "fastify": "^5.6.1",
    "fastify-plugin": "^5.1.0",
    "ioredis": "^5.8.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@modelcontextprotocol/inspector": "^0.17.2",
    "@radix-ui/themes": "^3.2.1",
    "@tailwindcss/postcss": "^4.1.16",
    "@tanstack/react-table": "^8.21.3",
    "@testing-library/react": "^16.3.0",
    "@types/express-session": "^1.18.2",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@types/ws": "^8.18.1",
    "@vitejs/plugin-react": "^5.1.0",
    "clsx": "^2.1.1",
    "dotenv": "^17.2.3",
    "eslint": "^9.38.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "happy-dom": "^20.0.8",
    "husky": "^9.1.7",
    "i18next": "^25.6.0",
    "i18next-browser-languagedetector": "^8.2.0",
    "jotai": "^2.15.0",
    "ky": "^1.13.0",
    "lint-staged": "^16.2.6",
    "pino-pretty": "^13.1.2",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.7.1",
    "prisma": "^6.18.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-i18next": "16.1.6",
    "react-router": "^7.9.4",
    "rimraf": "^6.0.1",
    "sortier": "^2.1.1",
    "tailwindcss": "^4.1.16",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.2",
    "vite": "7.1.12",
    "vitest": "^4.0.2"
  },
  "engines": {
    "node": ">=20.11.0",
    "npm": ">=10.0.0"
  }
}
