{
  "name": "node-short-link",
  "version": "0.1.1",
  "description": "Node Short link service",
  "main": "dist/server/index.js",
  "type": "module",
  "scripts": {
    "build": "npm run build:browser && npm run build:server",
    "build:browser": "vite build",
    "build:server": "tsc --project src/server/tsconfig.json",
    "clean": "rimraf dist",
    "husky:pre-push": "npm test",
    "husky:pre-commit": "npx --no lint-staged",
    "husky:commit-msg": "npx --no commitlint -- --edit",
    "start:server": "node -r dotenv/config ./dist/index.js",
    "start:browser": "vite dev",
    "watch:build:server": "npm run build:server -- --watch --incremental --sourceMap --preserveWatchOutput",
    "test": "vitest run",
    "prepare": "npm run clean && npx --no prisma generate && npm run build && npx --no husky install"
  },
  "keywords": [
    "node",
    "short-link"
  ],
  "author": "Han Jiawei",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^5.5.2",
    "connect-redis": "^7.1.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-openapi": "^12.1.3",
    "express-session": "^1.17.3",
    "i18next": "^23.7.3",
    "ioredis": "^5.3.2",
    "mime": "^3.0.0",
    "pino-http": "^8.5.1",
    "swagger-ui-express": "^5.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.4.1",
    "@commitlint/config-conventional": "^18.4.0",
    "@mui/base": "^5.0.0-beta.19",
    "@tanstack/react-table": "^8.10.7",
    "@testing-library/react": "^14.1.0",
    "@types/express": "^4.17.17",
    "@types/express-session": "^1.17.7",
    "@types/mime": "^3.0.2",
    "@types/react": "^18.2.33",
    "@types/react-dom": "^18.2.14",
    "@types/supertest": "^2.0.12",
    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "@vitejs/plugin-react": "^4.1.1",
    "autoprefixer": "^10.4.16",
    "clsx": "^2.0.0",
    "eslint": "^8.53.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-react": "^7.33.2",
    "happy-dom": "^12.9.0",
    "husky": "^8.0.3",
    "jotai": "^2.5.1",
    "ky": "^1.1.3",
    "lint-staged": "^15.1.0",
    "pino-pretty": "^10.2.3",
    "postcss": "^8.4.31",
    "postcss-import": "^15.1.0",
    "prettier": "^3.0.3",
    "prisma": "^5.5.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.18.0",
    "rimraf": "^5.0.5",
    "sortier": "^2.0.2",
    "supertest": "^6.3.3",
    "tailwindcss": "^3.3.5",
    "typescript": "^5.2.2",
    "vite": "4.5.0",
    "vitest": "^0.34.6"
  },
  "engines": {
    "node": ">=20.9.0",
    "npm": ">=10.0.0"
  }
}
