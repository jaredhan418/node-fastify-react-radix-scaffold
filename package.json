{
  "name": "node-short-link",
  "version": "0.1.3",
  "description": "Node Short link service",
  "main": "dist/server/index.js",
  "type": "module",
  "scripts": {
    "build": "npm run build:browser && npm run build:server",
    "build:browser": "vite build",
    "build:server": "tsc --project src/server/tsconfig.json",
    "clean": "rimraf dist",
    "husky:pre-push": "npm test",
    "husky:pre-commit": "npx --no lint-staged",
    "husky:commit-msg": "npx --no commitlint -- --edit",
    "start:server": "node --env-file=.env ./dist/index.js",
    "start:browser": "vite dev",
    "watch:build:server": "npm run build:server -- --watch --incremental --sourceMap --preserveWatchOutput",
    "test": "vitest run",
    "prepare": "npm run clean && npx --no prisma generate && npm run build && npx --no husky install"
  },
  "keywords": [
    "node",
    "short-link"
  ],
  "author": "Han Jiawei",
  "license": "MIT",
  "dependencies": {
    "@fastify/compress": "^7.0.0",
    "@fastify/cookie": "^9.3.1",
    "@fastify/http-proxy": "^9.4.0",
    "@fastify/session": "^10.7.0",
    "@fastify/static": "^7.0.0",
    "@fastify/swagger": "^8.14.0",
    "@fastify/swagger-ui": "^2.1.0",
    "@prisma/client": "^5.9.1",
    "connect-redis": "^7.1.1",
    "fastify": "^4.26.0",
    "fastify-plugin": "^4.5.1",
    "ioredis": "^5.3.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.6.0",
    "@commitlint/config-conventional": "^18.6.0",
    "@radix-ui/themes": "^2.0.3",
    "@tanstack/react-table": "^8.11.8",
    "@testing-library/react": "^14.2.1",
    "@types/express-session": "^1.17.10",
    "@types/react": "^18.2.51",
    "@types/react-dom": "^18.2.18",
    "@types/supertest": "^6.0.2",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^6.20.0",
    "@typescript-eslint/parser": "^6.20.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.17",
    "clsx": "^2.1.0",
    "dotenv": "^16.4.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.33.2",
    "happy-dom": "^13.3.8",
    "husky": "^9.0.10",
    "i18next": "^23.8.2",
    "jotai": "^2.6.4",
    "ky": "^1.2.0",
    "lint-staged": "^15.2.1",
    "pino-pretty": "^10.3.1",
    "postcss": "^8.4.33",
    "postcss-import": "^16.0.0",
    "prettier": "^3.2.4",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "prisma": "^5.9.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.22.0",
    "rimraf": "^5.0.5",
    "sortier": "^2.0.2",
    "supertest": "^6.3.4",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3",
    "vite": "5.0.12",
    "vitest": "^1.2.2"
  },
  "engines": {
    "node": ">=20.9.0",
    "npm": ">=10.0.0"
  }
}
