{
  "name": "node-fastify-react-radix-scaffold",
  "version": "0.3.0",
  "description": "Node fastify Prisma scaffold",
  "main": "dist/server/index.js",
  "type": "module",
  "scripts": {
    "build": "npm run build:browser && npm run build:server",
    "build:browser": "vite build",
    "build:server": "tsc --project src/server/tsconfig.json",
    "clean": "rimraf dist",
    "husky:pre-push": "npm test",
    "husky:pre-commit": "npx --no lint-staged",
    "husky:commit-msg": "npx --no commitlint -- --edit",
    "start:server": "node --env-file=.env ./dist/index.js",
    "start:browser": "vite dev",
    "watch:build:server": "npm run build:server -- --watch --incremental --sourceMap --preserveWatchOutput",
    "test": "vitest run",
    "prepare": "npm run clean && npx --no prisma generate && npm run build && npx --no husky"
  },
  "keywords": [
    "node",
    "short-link"
  ],
  "author": "Han Jiawei",
  "license": "MIT",
  "dependencies": {
    "@fastify/compress": "^8.0.1",
    "@fastify/cookie": "^11.0.1",
    "@fastify/http-proxy": "^11.0.0",
    "@fastify/session": "^11.0.1",
    "@fastify/static": "^8.0.3",
    "@fastify/swagger": "^9.4.0",
    "@fastify/swagger-ui": "^5.0.1",
    "@prisma/client": "^5.22.0",
    "connect-redis": "^8.0.0",
    "fastify": "^5.1.0",
    "fastify-plugin": "^5.0.1",
    "ioredis": "^5.4.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.6.0",
    "@commitlint/config-conventional": "^19.6.0",
    "@radix-ui/themes": "^3.1.6",
    "@tanstack/react-table": "^8.20.5",
    "@testing-library/react": "^16.0.1",
    "@types/express-session": "^1.18.1",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@types/ws": "^8.5.13",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.20",
    "clsx": "^2.1.1",
    "dotenv": "^16.4.5",
    "eslint": "^9.15.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.2",
    "happy-dom": "^15.11.7",
    "husky": "^9.1.7",
    "i18next": "^24.0.2",
    "i18next-browser-languagedetector": "^8.0.0",
    "jotai": "^2.10.3",
    "ky": "^1.7.2",
    "lint-staged": "^15.2.10",
    "oxlint": "^0.13.2",
    "pino-pretty": "^13.0.0",
    "postcss": "^8.4.49",
    "postcss-import": "^16.1.0",
    "prettier": "^3.4.1",
    "prettier-plugin-tailwindcss": "^0.6.9",
    "prisma": "^5.22.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-i18next": "15.1.2",
    "react-router": "^7.0.1",
    "rimraf": "^6.0.1",
    "sortier": "^2.0.2",
    "tailwindcss": "^3.4.15",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.16.0",
    "vite": "6.0.1",
    "vitest": "^2.1.6"
  },
  "engines": {
    "node": ">=20.11.0",
    "npm": ">=10.0.0"
  }
}
