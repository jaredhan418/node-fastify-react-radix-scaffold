{
  "name": "node-fastify-react-radix-scaffold",
  "version": "0.4.0",
  "description": "Node fastify Prisma scaffold",
  "main": "dist/server/index.js",
  "type": "module",
  "scripts": {
    "build": "npm run build:browser && npm run build:server",
    "build:browser": "vite build",
    "build:server": "tsc --project src/server/tsconfig.json",
    "clean": "rimraf dist",
    "husky:pre-push": "npm test",
    "husky:pre-commit": "npx --no lint-staged",
    "husky:commit-msg": "npx --no commitlint -- --edit",
    "start:server": "node --env-file=.env ./dist/index.js",
    "start:browser": "vite dev",
    "watch:build:server": "npm run build:server -- --watch --incremental --sourceMap --preserveWatchOutput",
    "test": "vitest run",
    "prepare": "npm run clean && npx --no prisma generate && npm run build && npx --no husky"
  },
  "keywords": [
    "node",
    "short-link"
  ],
  "author": "Han Jiawei",
  "license": "MIT",
  "dependencies": {
    "@fastify/compress": "^8.0.1",
    "@fastify/cookie": "^11.0.2",
    "@fastify/http-proxy": "^11.1.2",
    "@fastify/session": "^11.1.0",
    "@fastify/static": "^8.1.1",
    "@fastify/swagger": "^9.4.2",
    "@fastify/swagger-ui": "^5.2.2",
    "@prisma/client": "^6.4.1",
    "connect-redis": "^8.0.1",
    "fastify": "^5.2.1",
    "fastify-plugin": "^5.0.1",
    "ioredis": "^5.5.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.7.1",
    "@commitlint/config-conventional": "^19.7.1",
    "@radix-ui/themes": "^3.2.1",
    "@tailwindcss/postcss": "^4.0.9",
    "@tanstack/react-table": "^8.21.2",
    "@testing-library/react": "^16.2.0",
    "@types/express-session": "^1.18.1",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@types/ws": "^8.5.14",
    "@vitejs/plugin-react": "^4.3.4",
    "clsx": "^2.1.1",
    "dotenv": "^16.4.7",
    "eslint": "^9.21.0",
    "eslint-config-prettier": "^10.0.2",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-react": "^7.37.4",
    "happy-dom": "^17.1.8",
    "husky": "^9.1.7",
    "i18next": "^24.2.2",
    "i18next-browser-languagedetector": "^8.0.4",
    "jotai": "^2.12.1",
    "ky": "^1.7.5",
    "lint-staged": "^15.4.3",
    "pino-pretty": "^13.0.0",
    "postcss": "^8.5.3",
    "prettier": "^3.5.2",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "prisma": "^6.4.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-i18next": "15.4.1",
    "react-router": "^7.2.0",
    "rimraf": "^6.0.1",
    "sortier": "^2.1.0",
    "tailwindcss": "^4.0.9",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.25.0",
    "vite": "6.2.0",
    "vitest": "^3.0.7"
  },
  "engines": {
    "node": ">=20.11.0",
    "npm": ">=10.0.0"
  }
}
